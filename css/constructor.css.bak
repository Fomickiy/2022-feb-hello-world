.possibility {
  position: relative;
}

.possibility__inner {
  position: relative;
  padding-top: 125px;
  padding-bottom: 100px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.possibility__inner .advantage-stars__1 {
  position: absolute;
  top: 25px;
  left: 34%;
  -webkit-animation: floatStar 10s linear infinite;
  animation: floatStar 10s linear infinite;
  z-index: -1;
}

.advantage__inner .advantage-stars__2 {
  position: absolute;
  top: 10%;
  left: 24%;
  -webkit-animation: pulse 9s linear infinite;
  animation: pulse 9s linear infinite;
}

.star-red-23 {
  width: 23px;
  height: 23px;
  border-radius: 100%;
  background-color: #f110ba;
}

.advantage-stars__2 {
  position: absolute;
  top: 70px;
  right: 23%;
  -webkit-animation: floatStar 11s linear infinite;
  animation: floatStar 11s linear infinite;
}


.advantage__title {
  max-width: 600px;
  margin-bottom: 57px;
}

.title {
  font-size: 45px;
  font-weight: 300;
  letter-spacing: 8px;
  line-height: 1.3;
  text-transform: uppercase;
}

.advantage__items {
  display: grid;
  grid-template-columns: repeat(2, 1fr);
  grid-template-rows: repeat(2, 1fr);
  grid-row-gap: 100px;
  grid-column-gap: 30px;
}

.advantage__item-text {
  font-size: 20px;
  line-height: 130%;
}

.advantage__item-img {
  font-size: 30px;
  margin-bottom: 25px;
}

/*tarrifs*/
.tariffs__inner {
  position: relative;
  padding: 70px 0;
}

.tariffs__inner .tariffs-stars__1 {
  position: absolute;
  top: 75px;
  left: 34%;
  -webkit-animation: floatStar 30s linear infinite;
  animation: floatStar 30s linear infinite;
  z-index: -1;
}

.tariffs__inner .tariffs-stars__2 {
  position: absolute;
  top: 170px;
  right: 23%;
  -webkit-animation: floatStar 11s linear infinite;
  animation: floatStar 11s linear infinite;
}

.tariffs__title {
  max-width: 865px;
  margin-bottom: 20px;
  margin-left: 450px;
}

.tariffs__slider-item-text {
  font-size: 20px;
  line-height: 1.2;
}

.tariffs__slider {
  width: 100px;
}
.background-site {
  position: fixed;
  top: 0;
  left: 0;
  min-width: 100vw;
  min-height: 100vh;
  background-image: url(../img/mission/space_bg.png);
  background-position: center;
  background-repeat: repeat;
  background-size: cover;
  opacity: .7;
  z-index: -100;
  background-color: #060e28;
}

.container {
  max-width: 1410px;
  padding: 0 15px;
  margin: 0 auto
}

/*Nav*/
.navigetion {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-top: 40px;
  padding-bottom: 20px
}

@media(max-width:1299.98px) {
  .navigetion {
    padding-top: 30px
  }
}


/*Menu*/

.footer__links .social__menu {
  display: flex;
  justify-content: flex-start;
  margin-top: 20px;
  margin-bottom: 30px
}

@media(max-width:991.98px) {
  .footer__links .social__menu {
    justify-content: center
  }
}

.footer__links .social__menu-item {
  opacity: .6;
  transition: all .3s ease
}

.footer__links .social__menu-item+.social__menu-item {
  margin-left: 27px
}

.footer__links .social__menu-item:hover {
  opacity: 1
}





